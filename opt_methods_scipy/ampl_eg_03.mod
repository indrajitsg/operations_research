# -------------------------------------------------------------------
# Quadratic Programming
# minimize:  f = 2*x1**2 + x2**2 + x1*x2 + x1 + x2

# subject to:
#            x1 >= 0
#            x2 >= 0
#            x1 + x2 == 1
# -------------------------------------------------------------------

var x1 >= 0;
var x2 >= 0;

minimize obj:
	2 * x1^2 + x2^2 + x1*x2 + x1 + x2;

s.t. c1:
	x1 + x2 == 1;

